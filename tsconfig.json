{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "api/*": ["./api/*"],
      "App/*": ["./App/*"],
      "components/*": ["./components/*"],
      "enums/*": ["./enums/*"],
      "layout/*": ["./layout/*"],
      "pages/*": ["./pages/*"],
      "types/*": ["./types/*"],
      "utils/*": ["./utils/*"],
      },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "./types"],
}
